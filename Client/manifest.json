{
    "manifest_version": 2,
    "name": "Distraction Blocker",
    "version": "1.0",
    "description": "Block sites that distract you.",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
        "scripts": ["background.js"]
    },
  "browser_action": {
    "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}
